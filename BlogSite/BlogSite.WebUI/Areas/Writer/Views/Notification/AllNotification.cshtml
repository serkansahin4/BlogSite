@model IEnumerable<Notification>
@{
    ViewData["Title"] = "AllNotification";
    Layout = "~/Areas/Writer/Views/Shared/_WriterLayout.cshtml";
}

<h1>Bildirimler</h1>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>#</th>
                <th>Bildirm</th>
                <th>Tarih</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
             {
            <tr>
                <td>@item.Id</td>
                <td>@item.Details</td>
                <td>@item.Date</td>
            </tr>
            }
            
        </tbody>
    </table>

